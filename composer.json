{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "arhitector/yandex": "dev-master",
        "astrotomic/laravel-translatable": "^11.9",
        "cviebrock/eloquent-sluggable": "^8.0",
        "divineomega/laravel-last-activity": "^1.3",
        "doctrine/dbal": "^3.3",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.3",
        "intervention/image": "^2.5",
        "kalnoy/nestedset": "^5.0",
        "laravel/framework": "^8.40",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.2",
        "livewire/livewire": "^2.10",
        "maatwebsite/excel": "^3.1",
        "rappasoft/laravel-livewire-tables": "^1.22",
        "realrashid/sweet-alert": "^4.1",
        "spatie/array-to-xml": "^3.0",
        "spatie/laravel-backup": "^7.6",
        "spatie/laravel-medialibrary": "^9.8",
        "spatie/laravel-permission": "^5.1",
        "spatie/laravel-query-builder": "^3.5",
        "spatie/laravel-settings": "^2.3",
        "yajra/laravel-datatables": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "beyondcode/laravel-query-detector": "^1.5",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "kitloong/laravel-migrations-generator": "^5.2",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
